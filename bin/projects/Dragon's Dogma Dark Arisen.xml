<project>
	<name>Dragon's Dogma Dark Arisen</name>
	<dependencies>
		<dependency>Shared</dependency>
	</dependencies>
	<settings>
		<setting name="archive_endian">Little</setting>
		<setting name="archive_version">7</setting>
		<setting name="archive_compression_scheme">Zlib</setting>
		<setting name="archive_same_size_is_uncompressed">False</setting>
	</settings>
	<list_location>Dragon's Dogma Dark Arisen</list_location>
	<install_locations>
