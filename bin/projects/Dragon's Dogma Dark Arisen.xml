			<action type="registry" key="HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\Steam App 367500" value="InstallLocation" />
		</install_location>
		<install_location>
			<action type="registry" key="HKEY_LOCAL_MACHINE\SOFTWARE\Wow6432Node\Microsoft\Windows\CurrentVersion\Uninstall\Steam App 367500" value="InstallLocation" />
		</install_location>
		<install_location>
			<action type="registry" key="HKEY_LOCAL_MACHINE\SOFTWARE\Valve\Steam" value="InstallPath" />
			<action type="combine">steamapps</action>
			<action type="combine">common</action>
			<action type="combine">DDDA</action>
		</install_location>
		<install_location>
			<action type="registry" key="HKEY_LOCAL_MACHINE\SOFTWARE\Wow6432Node\Valve\Steam" value="InstallPath" />
			<action type="combine">steamapps</action>
			<action type="combine">common</action>
